    %jmp T_0.8;
T_0.7 ;
    %load/vec4 v0x55002af710_0;
    %pad/u 8;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %load/vec4 v0x55002fc140_0;
    %ix/load 4, 1, 0;
