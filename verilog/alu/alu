    %pad/u 8;
    %load/vec4 v0x55002fbfa0_0;
    %pad/u 8;
    %or;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %jmp T_0.8;
T_0.4 ;
    %pushi/vec4 15, 0, 8;
    %load/vec4 v0x55002af710_0;
    %pad/u 8;
    %xor;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %jmp T_0.8;
T_0.5 ;
    %pushi/vec4 15, 0, 8;
    %load/vec4 v0x55002af710_0;
    %pad/u 8;
    %xor;
    %addi 1, 0, 8;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %jmp T_0.8;
T_0.6 ;
    %load/vec4 v0x55002af710_0;
