    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %jmp T_0.8;
T_0.0 ;
    %load/vec4 v0x55002af710_0;
    %pad/u 8;
    %load/vec4 v0x55002fbfa0_0;
    %pad/u 8;
    %add;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %jmp T_0.8;
T_0.1 ;
    %load/vec4 v0x55002af710_0;
    %pad/u 8;
    %load/vec4 v0x55002fbfa0_0;
    %pad/u 8;
    %sub;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %jmp T_0.8;
T_0.2 ;
    %load/vec4 v0x55002af710_0;
    %pad/u 8;
    %load/vec4 v0x55002fbfa0_0;
    %pad/u 8;
    %and;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %jmp T_0.8;
T_0.3 ;
