    %shiftr 4;
    %store/vec4 v0x55002fc140_0, 0, 8;
    %jmp T_0.8;
T_0.8 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55002d5290;
T_1 ;
    %vpi_func 2 11 "$value$plusargs" 32, "a=%d", v0x55002fc2d0_0 {0 0 0};
    %nor/r;
