S_0x55002d5290 .scope module, "alu_tb" "alu_tb" 2 1;
 .timescale 0 0;
v0x55002fc2d0_0 .var "a", 3 0;
v0x55002fc3b0_0 .var "b", 3 0;
v0x55002fc480_0 .var "s", 2 0;
v0x55002fc580_0 .net "y", 7 0, v0x55002fc140_0;  1 drivers
E_0x55002e49c0 .event edge, v0x55002fc140_0;
S_0x55002af4b0 .scope module, "a1" "alu" 2 6, 3 2 0, S_0x55002d5290;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a"
