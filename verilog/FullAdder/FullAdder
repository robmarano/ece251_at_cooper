#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55002af050 .scope module, "FullAdder_tb" "FullAdder_tb" 2 3;
 .timescale -9 -12;
v0x55002f9530_0 .var "Cin", 0 0;
v0x55002f95f0_0 .net "Cout", 0 0, L_0x55002f9f90;  1 drivers
v0x55002f9690_0 .var "In1", 0 0;
v0x55002f9790_0 .var "In2", 0 0;
v0x55002f9860_0 .net "Sum", 0 0, L_0x55002c3680;  1 drivers
v0x55002f9900_0 .var "i", 2 0;
S_0x55002af1d0 .scope module, "uut" "FullAdder" 2 18, 3 3 0, S_0x55002af050;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "In1"
    .port_info 1 /INPUT 1 "In2"
    .port_info 2 /INPUT 1 "Cin"
    .port_info 3 /OUTPUT 1 "Sum"
    .port_info 4 /OUTPUT 1 "Cout"
L_0x55002c3610 .functor XOR 1, v0x55002f9690_0, v0x55002f9790_0, C4<0>, C4<0>;
L_0x55002c3680 .functor XOR 1, L_0x55002c3610, v0x55002f9530_0, C4<0>, C4<0>;
L_0x55002f9b60 .functor AND 1, v0x55002f9690_0, v0x55002f9790_0, C4<1>, C4<1>;
L_0x55002f9bd0 .functor AND 1, v0x55002f9790_0, v0x55002f9530_0, C4<1>, C4<1>;
L_0x55002f9d00 .functor OR 1, L_0x55002f9b60, L_0x55002f9bd0, C4<0>, C4<0>;
L_0x55002f9dc0 .functor AND 1, v0x55002f9530_0, v0x55002f9690_0, C4<1>, C4<1>;
L_0x55002f9f90 .functor OR 1, L_0x55002f9d00, L_0x55002f9dc0, C4<0>, C4<0>;
v0x55002e6c30_0 .net "Cin", 0 0, v0x55002f9530_0;  1 drivers
v0x55002f8d00_0 .net "Cout", 0 0, L_0x55002f9f90;  alias, 1 drivers
v0x55002f8dc0_0 .net "In1", 0 0, v0x55002f9690_0;  1 drivers
v0x55002f8e60_0 .net "In2", 0 0, v0x55002f9790_0;  1 drivers
v0x55002f8f20_0 .net "Sum", 0 0, L_0x55002c3680;  alias, 1 drivers
v0x55002f9030_0 .net *"_s0", 0 0, L_0x55002c3610;  1 drivers
v0x55002f9110_0 .net *"_s10", 0 0, L_0x55002f9dc0;  1 drivers
v0x55002f91f0_0 .net *"_s4", 0 0, L_0x55002f9b60;  1 drivers
v0x55002f92d0_0 .net *"_s6", 0 0, L_0x55002f9bd0;  1 drivers
v0x55002f93b0_0 .net *"_s8", 0 0, L_0x55002f9d00;  1 drivers
    .scope S_0x55002af050;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x55002f9900_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x55002af050;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55002f9690_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55002f9790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55002f9530_0, 0, 1;
    %vpi_call 2 26 "$monitor", "In1=0x%0h In2=0x%0h Cin=0x%0h Cout=0x%0h Sum=0x%0h", v0x55002f9690_0, v0x55002f9790_0, v0x55002f9530_0, v0x55002f95f0_0, v0x55002f9860_0 {0 0 0};
    %delay 100000, 0;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x55002f9900_0, 0, 3;
T_1.0 ;
    %load/vec4 v0x55002f9900_0;
    %pad/u 32;
    %cmpi/u 8, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x55002f9690_0;
    %load/vec4 v0x55002f9790_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x55002f9530_0;
    %concat/vec4; draw_concat_vec4
    %addi 1, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x55002f9530_0, 0, 1;
    %split/vec4 1;
    %store/vec4 v0x55002f9790_0, 0, 1;
    %store/vec4 v0x55002f9690_0, 0, 1;
    %delay 20000, 0;
    %load/vec4 v0x55002f9900_0;
    %addi 1, 0, 3;
    %store/vec4 v0x55002f9900_0, 0, 3;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
